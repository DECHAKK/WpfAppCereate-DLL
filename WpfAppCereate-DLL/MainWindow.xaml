<Window x:Class="WpfAppCereate_DLL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppCereate_DLL"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBox x:Name="txtRedian" Grid.Row="0" Height="40" Width=" 150"/>
        <TextBox x:Name="txtArea" Grid.Row="1" Height="40" Width="150"/>
        <Button x:Name="calculate" Grid.Row="2" Height="40" Width="150" Content="Calculate" Click="calculate_Click"/>
    </Grid>
</Window>
